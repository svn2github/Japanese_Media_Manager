          <InfoImage flipY="true" diffuse="diffusemirrorwide.png">
          </InfoImage>
          <imageFolder>-</imageFolder>
          <imageFolderFocus>thumbborder.png</imageFolderFocus>
          <imageFolderFocus>-</imageFolderFocus>
